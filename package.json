{
  "name": "fhirformjs",
  "description": "FHIR Questionnaire To Form Converter for rendering",
  "version": "1.1.4",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "author": {
    "name": "Bell Eapen",
    "email": "gravatar@gulfdoctor.net",
    "url": "https://nuchange.ca"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dermatologist/fhirformjs.git"
  },
  "homepage": "https://github.com/dermatologist/fhirformjs",
  "keywords": [
    "fhir",
    "json-schema",
    "questionnaire",
    "forms",
    "ehealth"
  ],
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "test-watch": "tsdx test --watch",
    "lint": "tsdx lint --fix",
    "prepare": "tsdx build",
    "docs": "typedoc --out docs src",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "//": "pre-commit tsdx lint"
    }
  },
  "jest": {
      "moduleNameMapper": {
        "axios": "axios/dist/node/axios.cjs"
      }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "module": "dist/fhirformjs.esm.js",
  "size-limit": [
    {
      "path": "dist/fhirformjs.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/fhirformjs.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.0.1",
    "husky": "^9.0.7",
    "size-limit": "^11.0.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.6.2",
    "typedoc": "^0.23.28",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@ahryman40k/ts-fhir-types": "^4.0.39",
    "axios": "^1.6.2",
    "ts-jest": "^29.1.1",
    "uuidv4": "^6.2.13"
  }
}
